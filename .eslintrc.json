{
    "extends": "airbnb",
    "plugins": [
        "react-native"
    ],
    "rules": {
        "semi": "off",
        "import/extensions": "off",
        "global-require": "off",
        "react/jsx-filename-extension": "off",
        "arrow-parens": "off",
        "jsx-quotes": ["error", "prefer-single"],
        "prefer-template": "off"
    },
    "globals": {
        "fetch": false,
        "__DEV__": false
    }
}
